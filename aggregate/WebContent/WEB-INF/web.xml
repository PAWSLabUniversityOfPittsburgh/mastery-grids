<?xml version="1.0" encoding="UTF-8"?>
<web-app 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" 
    id="WebApp_ID" version="3.0">
  <display-name>AggregateServices</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>GetContentLevels</servlet-name>
    <servlet-class>GetContentLevels</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetContentLevels</servlet-name>
    <url-pattern>/GetContentLevels</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TrackAction</servlet-name>
    <servlet-class>TrackAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TrackAction</servlet-name>
    <url-pattern>/TrackAction</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>StoreFeedback</servlet-name>
    <servlet-class>StoreFeedback</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StoreFeedback</servlet-name>
    <url-pattern>/StoreFeedback</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RegisterGroup</servlet-name>
    <servlet-class>RegisterGroup</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterGroup</servlet-name>
    <url-pattern>/RegisterGroup</url-pattern>
  </servlet-mapping>
    
  <servlet>
    <servlet-name>SetNonStudents</servlet-name>
    <servlet-class>SetNonStudents</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SetNonStudents</servlet-name>
    <url-pattern>/SetNonStudents</url-pattern>
  </servlet-mapping>
  

</web-app>